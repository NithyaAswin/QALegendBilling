<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="com.qalegendbilling.listeners.TestListener" />
	</listeners>
	<test thread-count="5" name="chromeTest">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qalegendbilling.testscript.HomeTest">
				<methods>
					<include name="tc_004_verifyuserNavigationBacktoLoginpage" />
				</methods>
			</class>
		</classes>
	</test>
	<test thread-count="5" name="edgeTest">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.qalegendbilling.testscript.LoginTest">
				<methods>
					<include name="tc_001_verifyLoginPage_Credentials" />
				</methods>
			</class>
		</classes>
	</test>
</suite> <!-- Suite -->